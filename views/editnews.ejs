<h1>add news</h1>

<form action="/news/<%=event._id%>/?_method=PUT" method="POST">
    <input type="text" name="event[title]" value="<%= event.title%>">
    <input type="text" name="event[img]" value="<%= event.img%>"><br>
    <textarea rows="50" cols="100" name="event[content]"><%= event.content%></textarea><br>
    <input type="submit">
</form>